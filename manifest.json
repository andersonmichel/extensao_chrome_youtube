{
    "name": "Youtube Search in Channel",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://*.youtube.com/watch?v=*"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_title": "Youtube Options"
    }
}